top_srcdir=@top_srcdir@
top_builddir=../..
srcdir=@srcdir@
VPATH=.:@srcdir@

@COMMENCE@

docs:
	cp -fr $(srcdir)/GUI ./
	-find $(PWD) -name CVS -exec rm -rf {} \;

clean:
	rm -fr `ls | grep -v "Makefile"`

distclean: clean
	rm -fr GUI

install:
	mkdir -p $(docdir)
	cp -rf GUI $(docdir)
	-find $(PWD) -name CVS -exec rm -rf {} \;

uninstall:
	rm -rf $(docdir)/GUI
